{
    "name": "sorted-rename-tool",
    "version": "0.1.1",
    "main": "main.js",
    "scripts": {
        "dev": "electron . ",
        "build:win32": "electron-packager . sorted-rename-tool --out=../sorted-rename-tool-win32-ia32 --platform=win32 --arch=ia32 --electron-version=1.8.2 --asar --app-version=0.1.1 --build-version=0.0.2 --no-package-manager --overwrite --ignore='(node_modules|screenshots|.git)' --prune",
        "build:linux64": "electron-packager . sorted-rename-tool --out=../sorted-rename-tool-linux-x64 --platform=linux --arch=x64 --electron-version=1.8.2 --asar --app-version=0.1.1 --build-version=0.0.2 --no-package-manager --overwrite --ignore='(node_modules|screenshots|.git)' --prune"
    },
    "devDependencies": {
        "electron": "^1.8.2",
        "electron-packager": "^11.0.1"
    }
}
